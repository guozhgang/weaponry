		return qcSign;
	}

	public void setQcSign(String qcSign) {
		this.qcSign = qcSign;
	}

	public String getPcSign() {
		return pcSign;
	}

	public void setPcSign(String pcSign) {
		this.pcSign = pcSign;
	}

	public String getOwnerSign() {
